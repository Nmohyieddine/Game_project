<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Propositions.
    -->
    <changeSet id="20210727081544-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="reponses_id"
                                 baseTableName="propositions"
                                 constraintName="fk_propositions_reponses_id"
                                 referencedColumnNames="id"
                                 referencedTableName="reponses"/>

        <addForeignKeyConstraint baseColumnNames="questions_id"
                                 baseTableName="propositions"
                                 constraintName="fk_propositions_questions_id"
                                 referencedColumnNames="id"
                                 referencedTableName="questions"/>

    </changeSet>
</databaseChangeLog>
